// 0x00000000: lui x1, 0x10000    (Load 0x10000 into upper 20 bits of x1)
// 0x00000004: addi x1, x1, 0x5   (Add 5 to x1, result = 0x10005)
// 0x00000008: sw x1, 0(x0)       (Store x1 to address 0x10000000)
// 0x0000000C: lw x2, 0(x0)       (Load from address 0x10000000 into x2)
// 0x00000010: addi x3, x0, 0x10  (Load 10 into x3)
// 0x00000014: beq x1, x2, +8     (Branch if x1 == x2)
// 0x00000018: addi x4, x0, 0x20  (Load 20 into x4)
// 0x0000001C: jal x5, +4         (Jump and link)
// 0x00000020: addi x6, x0, 0x30  (Load 30 into x6)
// 0x00000024: addi x7, x0, 0x40  (Load 40 into x7)
// 0x00000028: jal x0, -4         (Jump back - infinite loop)

