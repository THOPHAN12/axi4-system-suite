@00000000
// Test Case: Memory Read/Write Intensive
// Tests RAM access patterns

13 00 00 00  // nop
93 00 10 00  // li x1, 1
13 01 20 00  // li x2, 2
93 01 30 00  // li x3, 3
13 02 40 00  // li x4, 4

// Write sequence to RAM
23 20 11 00  // sw x1, 0(x2)   @ 0x00000002
23 22 31 00  // sw x3, 4(x2)   @ 0x00000006
23 24 11 00  // sw x1, 8(x2)   @ 0x0000000A
23 26 31 00  // sw x3, 12(x2)  @ 0x0000000E
23 28 11 00  // sw x1, 16(x2)  @ 0x00000012
23 2A 31 00  // sw x3, 20(x2)  @ 0x00000016

// Read sequence from RAM
03 24 01 00  // lw x8, 0(x2)   @ 0x00000002
83 24 41 00  // lw x9, 4(x2)   @ 0x00000006
03 25 81 00  // lw x10, 8(x2)  @ 0x0000000A
83 25 C1 00  // lw x11, 12(x2) @ 0x0000000E
03 26 01 01  // lw x12, 16(x2) @ 0x00000012
83 26 41 01  // lw x13, 20(x2) @ 0x00000016

// Write again
23 20 41 00  // sw x4, 0(x2)
23 22 41 00  // sw x4, 4(x2)

// Read again
03 27 01 00  // lw x14, 0(x2)
83 27 41 00  // lw x15, 4(x2)

6f 00 00 00  // j 0 (loop)

